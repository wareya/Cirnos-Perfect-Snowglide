[application]

config/name="Cirno's Perfect Snowglide"
run/main_scene="res://scenes/Main Menu.tscn"
boot_splash/image="res://assets/loading splash.png"
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://icon.png"

[autoload]

EmitterFactory="*res://singletons/EmitterFactory.gd"
Manager="*res://singletons/Manager.tscn"
Savedata="*res://singletons/Savedata.gd"

[debug]

settings/fps/force_fps=120

[display]

window/size/width=1280
window/size/height=720
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/editor_input_map/plugin.cfg", "res://addons/rmsmartshape/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

